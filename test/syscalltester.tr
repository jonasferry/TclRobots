while {1} {
    set scanner [scanner 359 10]
    dputs "scanner: $scanner"

    set dsp [dsp]
    dputs "dsp: $dsp"

    set cannon [cannon 0 70]
    dputs "cannon: $cannon"

    set health [health]
    dputs "health: $health"

    set drive [drive 359 1]
    dputs "drive: $drive"

    set speed [speed]
    dputs "speed: $speed"

    set heat [heat]
    dputs "heat: $heat"

    set loc_x [loc_x]
    dputs "loc_x: $loc_x"

    set loc_y [loc_y]
    dputs "loc_y: $loc_y"

    set tick [tick]
    dputs "tick: $tick"

    set rand [rand 1000000000]
    dputs "rand: $rand"
}